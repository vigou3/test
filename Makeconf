# GNU and this distribution version number
EMACSVERSION=25.1
EMACSPATCHLEVEL=1
DISTVERSION=3

# Extensions version numbers
ESSVERSION=16.10
AUCTEXVERSION=11.89
ORGVERSION=9.0.2
POLYMODEVERSION=0.0.0
MARKDOWNMODEVERSION=0.0.0
EXECPATHVERSION=0.0.0
PSVNVERSION=0.0.0

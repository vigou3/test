# GNU and this distribution version number
EMACSVERSION=25.1
EMACSPATCHLEVEL=1
DISTVERSION=999

# Extensions version numbers
ESSVERSION=16.10
AUCTEXVERSION=11.89
ORGVERSION=9.0.2
#POLYMODEVERSION=$(shell git -C ../polymode show -s --format="%ci" HEAD | cut -d " " -f 1)
POLYMODEVERSION=0.0.0
MARKDOWNMODEVERSION=$(shell git -C ../markdown-mode describe --tags | cut -d v -f 1)
#EXECPATHVERSION=$(shell git -C ../exec-path-from-shell describe --tags | cut -d "-" -f 1-2)
EXECPATHVERSION=0.0.0
#PSVNVERSION=$(shell svn -v status ../emacs-svn/psvn.el | cut -d " " -f 16 -)
PSVNVERSION=0.0.0

# Version strings and file names
VERSION=${EMACSVERSION}$(if ${EMACSPATCHLEVEL},-${EMACSPATCHLEVEL},)-modified-${DISTVERSION}
DISTNAME=Emacs-${VERSION}
DMGFILE=Emacs-${EMACSVERSION}$(if ${EMACSPATCHLEVEL},-${EMACSPATCHLEVEL},)-universal.dmg
